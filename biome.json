{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"formatter": {
		"enabled": true,
		"ignore": ["./src/data"]
	},
	"organizeImports": { "enabled": true },
	"linter": {
		"enabled": true,
		"ignore": ["./src/data"],
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": "off"
			},
			"correctness": {
				"noUnusedImports": "error",
				"noUnusedVariables": "error"
			}
		}
	},
	"javascript": {
		"parser": {
			"unsafeParameterDecoratorsEnabled": true
		}
	}
}
